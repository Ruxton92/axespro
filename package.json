  "dependencies": {
    "backbone": "^1.1.2",
    "backbone-query-parameters": "git://github.com/jhudson8/backbone-query-parameters",
    "backbone-routing": "^0.1.0",
    "backbone-validation": "^0.11.3",
    "backbone.marionette": "^2.4.1",
    "backbone.radio": "^0.9.0",
    "backbone.storage": "^0.1.0",
    "backbone.syphon": "^0.5.0",
    "bootstrap": "^3.3.5",
    "cookies-js": "^1.2.1",
    "es6-promise": "^2.1.1",
    "font-awesome": "^4.3.0",
    "handlebars": "^2.0.0",
    "jquery": "^2.1.3",
    "lodash": "^3.6.0"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "browserify-swap": {
    "@packages": [
      "underscore"
    ],
    "dist": {
      "underscore.js$": "lodash"
    }
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "hbsfy",
        {
          "extensions": [
            "hbs"
          ]
        }
      ],
      "browserify-swap",
      "browserify-shim"
    ]
  }
}
